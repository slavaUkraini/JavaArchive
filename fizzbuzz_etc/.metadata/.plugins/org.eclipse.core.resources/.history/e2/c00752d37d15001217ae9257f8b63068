import java.io.IOException;

public class Fizz {
	int[] mass;
	
	public void fizzbuzz (int n) throws IOException {
		mass = new int[n];
		//createmass(n);
		for(int i = 1; i<=n; i++){
			if (i==5*(i / 5)){
				System.out.println("fizz");
			}
			else if (i==7*(i/5)){
				System.out.println("buzz");
			}
			else if (i==35*(i/35)){
				System.out.println("fizzbuzz");
			}
			else {System.out.println(i);}
		}
	}
}
