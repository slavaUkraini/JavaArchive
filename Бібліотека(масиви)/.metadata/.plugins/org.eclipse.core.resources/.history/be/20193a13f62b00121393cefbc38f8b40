import ua.com.vita.krainik.firstPackage.Array;
import ua.com.vita.krainik.firstPackage.DataInput;


public class Tester {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Array ar = new Array();
		ar.printmass();
		ar.delEnd();
		ar.delete(2);
		ar.delete(0);
		System.out.println("Введіть елементи");
		for (int i = 0; i<6; i++)
		ar.addEnd(DataInput.getInt());
		System.out.println("Видаляємо останній");
		ar.delEnd();
		System.out.println("Виводимо");
		ar.printmass();
		System.out.println("Видаляємо третій (з 0 рахунок)");
		ar.delete(3);
		System.out.println("Виводимо");
		ar.printmass();
		System.out.println("Видаляємо увесь");
		ar.clear();
		System.out.println("Виводимо");
		ar.printmass();
		System.out.println("Усьо поки");
	}

}
