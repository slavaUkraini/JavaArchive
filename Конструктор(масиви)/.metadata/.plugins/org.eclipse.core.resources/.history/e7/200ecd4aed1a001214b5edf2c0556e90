public class MyArray {
	//Коли поля просто, а коли privat?
	int[] mass;
	public MyArray(int size)throws IOException {
		for(int i = 1; i <= size; i++){
			//System.out.print("Введіть " + i + "-й елемент масиву: ");
			int temp = DataInput.getInt();
			mass[i] = temp;
		}
	}	
}